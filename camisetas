while True:
    n = int(input())
    if n == 0:
        break
    
    camisetas = []
    for i in range(n):
        nome = input()
        cor_tam = input().split()
        cor, tam = cor_tam[0], cor_tam[1]
        camisetas.append((nome, cor, tam))
    
    camisetas_ord = sorted(camisetas, key=lambda c: (c[1], c[2], c[0]))
    
    for c in camisetas_ord:
        print(c[1], c[2], c[0])
    
    print()
